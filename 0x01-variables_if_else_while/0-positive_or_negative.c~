#include <stdlib.h>
#include <time.h>
/**
 * main - code is here 
 * Return: returns zero if all is right 
 */

/* betty style doc for function main goes there */
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
if (n > 0)
printf("n is positive\n");
else
printf("n is negative\n");
if (n == 0)
printf("n is zero\n");
return (0);
}
